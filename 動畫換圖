void Blink(){
    display.clear();
    display.drawXbm(0,0, 128, 64, your_img1);
    display.display();
    delay(300);
    display.clear();
    display.drawXbm(0,0, 128, 64, your_img2);
    display.display();
    delay(200);
    display.clear();
    display.drawXbm(0,0, 128, 64, your_img3);
    display.display();
    delay(200);
    display.clear();
    display.drawXbm(0,0, 128, 64, your_img2);
    display.display();
    delay(200);
    display.clear();
    display.drawXbm(0,0, 128, 64, your_img1);
    display.display();
}//待機札眨眨眼
void smile(){
    display.clear();
    display.drawXbm(0,0, 128, 64, your_img4);
    display.display();
    delay(300);
    display.clear();
  display.drawXbm(0,0, 128, 64, your_img5);
    display.display();
    delay(300);
     display.clear();   
       display.drawXbm(0,0, 128, 64, your_img6);
    display.display();
    delay(300);
     display.clear();   
}

void setup() {
  // put your setup code here, to run once:
   //OLED define
   pinMode(14,INPUT);
   display.init();
   display.flipScreenVertically();
   display.setContrast(255); //數值介於0~255，調整對比
   display.clear();//清除螢幕和緩衝區(clear)
}

void loop() {
  // put your main code here, to run repeatedly:
  Blink();
  if(digitalRead(14)==HIGH){
  smile();
  }
}
